#include <iostream>;
using namespace std;

bool isPrime(int num) {
    if (num <= 1)
        return false;
    for (int i = 2; i < num; i++) {
        if (num % i == 0)
            return false;
    }
    return true;
}

int main() {
    int num;
    cout &#8203;`oaicite:{"index":1,"invalid_reason":"Malformed citation << \"Enter a number: \";\n    cin >>"}`&#8203; num;
    if (isPrime(num))
        cout << num << " is a prime number.";
    else
        cout << num << " is not a prime number.";
    return 0;
}
